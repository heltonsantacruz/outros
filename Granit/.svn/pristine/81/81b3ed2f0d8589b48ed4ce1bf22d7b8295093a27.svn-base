package br.com.granit.apresentacao.usuario;

import org.apache.struts.validator.ValidatorForm;

public class UsuarioForm extends ValidatorForm{
	
	
	
	private String login;
	private String nome;
	private String senha;
	private String confirmaSenha;
	private String perfil;
	private Integer idUsuario;
	private String perfilFormatado;
	private String paginaRetorno;
	
	
	
	
	public String getPaginaRetorno() {
		return paginaRetorno;
	}
	public void setPaginaRetorno(String paginaRetorno) {
		this.paginaRetorno = paginaRetorno;
	}
	public String getPerfilFormatado() {
		return perfilFormatado;
	}
	public void setPerfilFormatado(String perfilFormatado) {
		this.perfilFormatado = perfilFormatado;
	}
	public Integer getIdUsuario() {
		return idUsuario;
	}
	public void setIdUsuario(Integer idUsuario) {
		this.idUsuario = idUsuario;
	}
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	public String getPerfil() {
		return perfil;
	}
	public void setPerfil(String perfil) {
		this.perfil = perfil;
	}
	public String getConfirmaSenha() {
		return confirmaSenha;
	}
	public void setConfirmaSenha(String confirmaSenha) {
		this.confirmaSenha = confirmaSenha;
	}
	public String getLogin() {
		return login;
	}
	public void setLogin(String login) {
		this.login = login;
	}
	public String getSenha() {
		return senha;
	}
	public void setSenha(String senha) {
		this.senha = senha;
	}
	

}
